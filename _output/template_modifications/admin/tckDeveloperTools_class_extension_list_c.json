{
    "template": "class_extension_list",
    "description": "Show class extension execution order",
    "execution_order": 6119,
    "enabled": true,
    "action": "preg_replace",
    "find": "#{\\$extension\\.to_class\\|split_long\\(25\\)}.*?<\\/xf:cell>#si",
    "replace": "$0\n\n<xf:cell class=\"dataList-cell--number dataList-cell--hint\" dir=\"auto\">{$extension.execute_order|number}</xf:cell>"
}